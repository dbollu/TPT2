:import borf.macros

:global string Version
:global string Running

wakeup()

Running = {borf.package.name}
Version = {borf.package.version}

execute({borf.script.ctl})

waituntil(isTowerTesting())

execute({borf.script.era})

execute({borf.script.inf})
execute({borf.script.common})
execute({borf.script.other})

; This won't actually execute unless the run ends
; see the script itself for further information
execute({borf.script.stop})
